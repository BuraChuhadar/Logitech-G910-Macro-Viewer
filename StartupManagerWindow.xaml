<Window x:Class="G910_Logitech_Utilities.StartupManagerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Startup Manager" Height="200" Width="400">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Text="Manage Startup Settings" FontSize="16" FontWeight="Bold" Margin="0,0,0,20"/>
            <Button Name="EnableStartupButton" Content="Enable Startup" Width="150" Margin="0,0,0,10" Click="EnableStartupButton_Click"/>
            <Button Name="DisableStartupButton" Content="Disable Startup" Width="150" Click="DisableStartupButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
